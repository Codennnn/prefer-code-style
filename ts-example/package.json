{
  "name": "portal",
  "version": "0.1.0",
  "license": "MIT",
  "author": "LeoKu <czc12580520@gmail.com>",
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "analyze": "cross-env SCRIPT_TYPE=analyze craco build",
    "test": "craco test",
    "prepare": "husky install",
    "deps": "yarn upgrade-interactive --latest",
    "deploy": "push-dir --dir=build --branch=gh-pages --cleanup",
    "lint": "npm run lint:es && npm run lint:style && npm run lint:ts",
    "lint:fix": "npm run lint:es --fix && npm run lint:style --fix",
    "lint:es": "eslint \"**/*.{mjs,js,jsx,ts,tsx}\"",
    "lint:style": "stylelint \"./src/**/*.{css,scss}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck"
  },
  "lint-staged": {
    "*.{mjs,js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,less,scss}": [
      "stylelint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@icon-park/react": "^1.3.5",
    "@tailwindcss/postcss7-compat": "^2.0.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^4.16.13",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "apexcharts": "^3.28.1",
    "autoprefixer": "9",
    "axios": "^0.21.3",
    "classnames": "^2.3.1",
    "craco-antd": "^1.19.0",
    "dayjs": "^1.10.6",
    "js-cookie": "^3.0.1",
    "lodash.debounce": "^4.0.8",
    "mockjs": "^1.1.0",
    "node-sass": "^6.0.1",
    "postcss": "7",
    "react": "^17.0.2",
    "react-apexcharts": "^1.3.9",
    "react-dom": "^17.0.2",
    "react-full-screen": "^1.1.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.1.2",
    "react-perfect-scrollbar": "^1.5.8",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.3.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.2.4",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^16.7.10",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.3",
    "@types/react-helmet": "^6.1.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "cross-env": "^7.0.3",
    "eslint-plugin-tailwind": "^0.2.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "push-dir": "^0.4.1",
    "webpack-bundle-analyzer": "^4.4.1"
  }
}
